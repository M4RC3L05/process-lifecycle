{
  "name": "@m4rc3l05/process-lifecycle",
  "version": "0.1.1",
  "exports": "./src/mod.ts",
  "tasks": {
    "deps:lock": "deno cache --frozen=false --node-modules-dir **/*.ts",
    "deps": "deno cache --reload --frozen --node-modules-dir **/*.ts"
  },
  "publish": {
    "include": [
      "./src/*.ts",
      "LICENSE",
      "README.md",
      "deno.json",
      "deno.lock"
    ],
    "exclude": [
      "./src/*_test.ts",
      "./src/test_deps.ts"
    ]
  }
}
