{
  "name": "@m4rc3l05/process-lifecycle",
  "version": "0.1.1",
  "exports": "./src/mod.ts",
  "tasks": {
    "deps:check": "deno cache --frozen src/*.ts src/**/*.ts",
    "deps:lock": "deno cache --frozen=false src/*.ts src/**/*.ts",
    "deps": "deno cache --reload src/*.ts src/**/*.ts"
  },
  "publish": {
    "include": [
      "./src/*.ts",
      "LICENSE",
      "README.md",
      "deno.json"
    ],
    "exclude": [
      "./src/*_test.ts",
      "./src/test_deps.ts"
    ]
  }
}
